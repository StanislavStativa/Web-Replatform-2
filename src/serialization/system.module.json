      {
        "name": "ApiKeys",
        "path": "/sitecore/system/Settings/Services/API Keys/xmcloudpreview",
        "scope": "singleItem",
        "allowedPushOperations": "CreateOnly"
      },
      {
        "name": "Module",
        "path": "/sitecore/system/Settings/Feature/TTS",
