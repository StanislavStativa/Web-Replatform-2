      },
      {
        "name": "TailwindConfiguration",
        "path": "/sitecore/system/Settings/Feature/Experience Accelerator/GridTailwind",
        "scope": "ItemAndDescendants",
        "allowedPushOperations": "CreateUpdateAndDelete"
      },
      {
        "name": "BucketItemsRule",
        "path": "/sitecore/system/Settings/Buckets/Item Buckets Settings",
        "scope": "singleItem",
        "allowedPushOperations": "CreateAndUpdate"
      }
